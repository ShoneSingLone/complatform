  "module": "es2020",
  "version": "0.0.0",
  "scripts": {
    "usenpm": "npm i",
    "bs": "pnpm i",
    "start": "vite --open --host --port 3010",
    "build": "npm run genIconJSON && cross-env IS_DEV=PRD vite build",
    "build:lib:tui_comment": "tui 编辑器的lib生成",
    "build:lib:tui": "cross-env type=lib:tui vite build && node ./preprocess/ci/cleanTuiDist.js",
    "genIconJSON_comment": "在public下的icon svg资源，获取文件名，用于系统图标的下拉选项 ",
    "genIconJSON": "node ./preprocess/genIconJSON.js",
    "lint": "prettier --write ./src",
    "update:ui_comment": "更新UI库",
    "update:ui": "pnpm uninstall @ventose/ui && pnpm i @ventose/ui -S",
    "git:pull": "git pull gitee main & git pull github main",
    "git:push": "git push gitee main & git push github main",
    "preview": "vite preview"
  },
  "dependencies": {
    "@toast-ui/editor": "^3.2.2",
    "ant-design-vue": "^3.2.15",
    "axios": "^0.26.1",
    "chalk": "^5.2.0",
