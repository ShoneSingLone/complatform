    "dayjs": "^1.11.7",
    "generate-schema": "^2.6.0",
    "idb-keyval": "^6.2.0",
    "immer": "^9.0.19",
    "jquery": "^3.6.4",
    "json5": "^2.2.3",
    "jsondiffpatch": "^0.4.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "qs": "^6.11.1",
    "vue": "^3.2.47"
  },
